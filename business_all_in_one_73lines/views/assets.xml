    <template id="editor_js" name="Editor Js" inherit_id="web_editor.assets_editor">
        <xpath expr="." position="inside">
            <script src="/business_all_in_one_73lines/static/src/js/rte_options.js"
                    type="text/javascript"/>
        </xpath>
    </template>


